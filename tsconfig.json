{
  "compilerOptions": {
    "moduleResolution": "node",
    "noImplicitAny": false,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "lib": [ "es2017", "dom" ],
    "baseUrl": ".",
    "paths": {
      "@images/*": [ "resources/images/*" ],
      "@utils/*": [ "resources/utils/*" ],
      "@shared-utils/*": [ "resources/shared-utils/*" ],
      "@components/*": [ "resources/components/*" ],
      "@three/l3/*": ["resources/threejs/l3/*"],
      "@three/controls/*": ["resources/threejs/controls/*"],
      "@three/libs/*": ["resources/threejs/libs/*"],
      "@three/special-effects/*": ["resources/threejs/special-effects/*"],
    }
  }
}
